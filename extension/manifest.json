{
  "manifest_version": 3,
  "name": "Altera Recorder",
  "version": "0.1.0",
  "description": "Record and replay user actions. (Scaffold)",
  "action": {
    "default_title": "Altera Recorder",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["downloads", "storage", "tabs"]
}
