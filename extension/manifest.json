{
  "manifest_version": 3,
  "name": "Altera Recorder",
  "version": "0.1.0",
  "description": "Record and replay user actions. (Scaffold)",
  "action": {
    "default_title": "Altera Recorder"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["downloads", "storage", "tabs", "scripting", "activeTab", "windows"],
  "host_permissions": ["<all_urls>"]
}
